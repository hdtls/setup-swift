{
  "name": "setup-swift",
  "version": "1.0.0",
  "description": "setup Swift action",
  "private": true,
  "main": "lib/setup-swift.js",
  "scripts": {
    "build": "ncc build -o dist/setup src/setup-swift.ts",
    "format": "prettier --write \"{,!(node_modules)/**/}*.ts\"",
    "format-check": "prettier --check \"{,!(node_modules)/**/}*.ts\"",
    "release": "git add -f dist/setup/index.js",
    "test": "jest"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/swift-actions/setup-swift.git"
  },
  "keywords": [
    "actions",
    "swift",
    "setup"
  ],
  "author": "Junfeng Zhang",
  "license": "MIT",
  "dependencies": {
    "@actions/core": "^1.10.0",
    "@actions/exec": "^1.1.1",
    "@actions/io": "^1.1.2",
    "@actions/tool-cache": "^2.0.1",
    "getos": "^3.2.1",
    "semver": "^7.3.8"
  },
  "devDependencies": {
    "@types/getos": "^3.0.1",
    "@types/jest": "^27.5.0",
    "@types/node": "^18.11.0",
    "@types/semver": "^7.3.12",
    "@vercel/ncc": "^0.34.0",
    "jest": "^26.6.3",
    "prettier": "^2.7.1",
    "ts-jest": "^26.5.6",
    "typescript": "^4.8.4"
  }
}
